            "name": "Custom SQL Example",
            "description": "Example using custom SQL rule",
            "isActive": true,
            "rootGroup": {
                "id": "group-005",
                "type": "GROUP",
                "logic": "AND",
                "rules": [
                    {
                        "id": "sql-rule-001",
                        "type": "SQL_RULE",
                        "name": "Prior Experience Check",
                        "description": "Check if worker has prior experience on similar projects",
                        "sqlQuery": "SELECT 1 FROM worker_history wh\nWHERE wh.worker_id = :worker_id\nAND wh.project_type = :project_type\nAND wh.experience_years >= 2"
                    }
                ]
            },
            "createdAt": "2025-01-01T00:00:00Z",
            "updatedAt": "2025-01-01T00:00:00Z"
